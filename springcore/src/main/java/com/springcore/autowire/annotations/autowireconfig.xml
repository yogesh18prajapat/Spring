<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
   <context:annotation-config/>


 
 
	<bean class="com.springcore.autowire.annotations.Address" name="address1" >
		<property name="city" value="Bangalore"/>
		<property name="state" value="Karnataka"/>
	</bean>
	
   <bean class="com.springcore.autowire.annotations.Address" name="address2" >
		<property name="city" value="Hyderabad"/>
		<property name="state" value="Telangana"/>
	</bean>
	
	<bean class="com.springcore.autowire.annotations.Address" name="address3" >
		<property name="city" value="Jaipur"/>
		<property name="state" value="Rajasthan"/>
	</bean>
	

	<bean class="com.springcore.autowire.annotations.Employee" name="emp1"/>
 
 
 
 </beans>